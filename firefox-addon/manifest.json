{
  "description": "Native messaging add-on to interact with mozilla tabs and bookmarks",
  "manifest_version": 2,
  "name": "mozeidon",
  "version": "3.1",
  "icons": {
    "48": "icons/trident.svg"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "mozeidon-dev@ac.local",
      "strict_min_version": "140.0",
      "data_collection_permissions": {
        "required": ["none"],
        "technical_data": false,
        "interaction_data": false
      }
    }
  },
  "background": {
    "scripts": ["dist/background.js"]
  },
  "browser_action": {
    "default_icon": "icons/trident.svg"
  },
  "permissions": ["nativeMessaging", "tabs", "sessions", "bookmarks", "history", "tabGroups"]
}
