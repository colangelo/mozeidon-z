{
  "description": "Native messaging add-on to interact with mozilla tabs and bookmarks",
  "manifest_version": 2,
  "name": "Mozeidon Z",
  "version": "3.2.2",
  "icons": {
    "48": "icons/mazinger-Z-128.png",
    "128": "icons/mazinger-Z-128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "mozeidon-z@a-layer.io",
      "strict_min_version": "142.0",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  },
  "background": {
    "scripts": ["dist/background.js"]
  },
  "browser_action": {
    "default_icon": "icons/mazinger-Z-128.png"
  },
  "permissions": ["nativeMessaging", "tabs", "sessions", "bookmarks", "history", "tabGroups"]
}
